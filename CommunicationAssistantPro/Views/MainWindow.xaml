<Window x:Class="CommunicationAssistantPro.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        Title="call center - basra delight | نظام الاتصالات المتكامل" 
        Height="800" Width="1200"
        MinHeight="600" MinWidth="900"
        WindowStartupLocation="CenterScreen"
        Icon="icon.ico">
    
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
            </ResourceDictionary.MergedDictionaries>
            
            <!-- Custom Styles -->
            <Style x:Key="StatusCardStyle" TargetType="materialDesign:Card">
                <Setter Property="Padding" Value="20"/>
                <Setter Property="UniformCornerRadius" Value="10"/>
                <Setter Property="materialDesign:ElevationAssist.Elevation" Value="Dp4"/>
            </Style>
            
            <Style x:Key="CallStatusStyle" TargetType="TextBlock">
                <Style.Triggers>
                    <DataTrigger Binding="{Binding CallStatus}" Value="Idle">
                        <Setter Property="Foreground" Value="Green"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding CallStatus}" Value="Busy">
                        <Setter Property="Foreground" Value="Red"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding CallStatus}" Value="Ringing">
                        <Setter Property="Foreground" Value="Orange"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding CallStatus}" Value="OnCall">
                        <Setter Property="Foreground" Value="Blue"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding CallStatus}" Value="Offline">
                        <Setter Property="Foreground" Value="Gray"/>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
        </ResourceDictionary>
    </Window.Resources>
    
    <!-- Main Layout -->
    <Grid Background="{StaticResource BackgroundBrush}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="250"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        
        <!-- Header -->
        <Border Grid.ColumnSpan="2" 
                Background="{StaticResource PrimaryBrush}" 
                Height="80">
            <Grid Margin="30,0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                
                <!-- Logo and Title -->
                <StackPanel Grid.Column="0" 
                           Orientation="Horizontal" 
                           VerticalAlignment="Center">
                    <materialDesign:PackIcon Kind="Phone" 
                                            Width="40" Height="40" 
                                            Foreground="White" 
                                            Margin="0,0,15,0"/>
                    <StackPanel>
                        <TextBlock Text="call center - basra delight" 
                                  FontSize="20" 
                                  FontWeight="Bold" 
                                  Foreground="White"
                                  FontFamily="Tajawal"/>
                        <TextBlock Text="نظام الاتصالات المتكامل" 
                                  FontSize="14" 
                                  Foreground="#E0E0E0"
                                  FontFamily="Tajawal"/>
                    </StackPanel>
                </StackPanel>
                
                <!-- User Info -->
                <StackPanel Grid.Column="2" 
                           Orientation="Horizontal" 
                           VerticalAlignment="Center">
                    <StackPanel Margin="0,0,20,0">
                        <TextBlock x:Name="UserNameTextBlock"
                                 Text="{Binding CurrentUser.FullName}"
                                 FontSize="16" 
                                 FontWeight="Bold" 
                                 Foreground="White" 
                                 HorizontalAlignment="Right"/>
                        <TextBlock x:Name="ExtensionTextBlock"
                                 Text="{Binding CurrentUser.Extension}"
                                 FontSize="12" 
                                 Foreground="#E0E0E0" 
                                 HorizontalAlignment="Right"/>
                    </StackPanel>
                    <materialDesign:PackIcon Kind="AccountCircle" 
                                            Width="40" Height="40" 
                                            Foreground="White"/>
                </StackPanel>
            </Grid>
        </Border>
        
        <!-- Sidebar Navigation -->
        <Border Grid.Row="1" 
                Background="#F5F5F5" 
                BorderBrush="#E0E0E0" 
                BorderThickness="0,0,1,0">
            <ScrollViewer VerticalScrollBarVisibility="Auto">
                <StackPanel Margin="20,20,20,40">
                    <!-- Main Navigation -->
                    <TextBlock Text="القائمة الرئيسية" 
                              FontSize="14" 
                              FontWeight="Bold" 
                              Foreground="#757575" 
                              Margin="0,0,0,15"/>
                    
                    <Button x:Name="DashboardButton"
                           Content="لوحة التحكم"
                           Style="{StaticResource MaterialDesignRaisedButton}"
                           Height="50" 
                           Margin="0,0,0,10"
                           Command="{Binding NavigateCommand}"
                           CommandParameter="Dashboard"
                           HorizontalAlignment="Stretch">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Border Background="{TemplateBinding Background}"
                                       BorderBrush="{TemplateBinding BorderBrush}"
                                       BorderThickness="1"
                                       CornerRadius="8">
                                    <StackPanel Orientation="Horizontal" 
                                              HorizontalAlignment="Center"
                                              VerticalAlignment="Center">
                                        <materialDesign:PackIcon Kind="ViewDashboard" 
                                                                Width="20" Height="20" 
                                                                Margin="0,0,10,0"/>
                                        <TextBlock Text="{TemplateBinding Content}" 
                                                  FontSize="14" 
                                                  FontWeight="Medium"/>
                                    </StackPanel>
                                </Border>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>
                    
                    <Button x:Name="CallsButton"
                           Content="إدارة المكالمات"
                           Style="{StaticResource MaterialDesignOutlinedButton}"
                           Height="50" 
                           Margin="0,0,0,10"
                           Command="{Binding NavigateCommand}"
                           CommandParameter="Calls"
                           HorizontalAlignment="Stretch">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Border Background="{TemplateBinding Background}"
                                       BorderBrush="{TemplateBinding BorderBrush}"
                                       BorderThickness="1"
                                       CornerRadius="8">
                                    <StackPanel Orientation="Horizontal" 
                                              HorizontalAlignment="Center"
                                              VerticalAlignment="Center">
                                        <materialDesign:PackIcon Kind="Phone" 
                                                                Width="20" Height="20" 
                                                                Margin="0,0,10,0"/>
                                        <TextBlock Text="{TemplateBinding Content}" 
                                                  FontSize="14" 
                                                  FontWeight="Medium"/>
                                    </StackPanel>
                                </Border>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>
                    
                    <Button x:Name="UsersButton"
                           Content="إدارة المستخدمين"
                           Style="{StaticResource MaterialDesignOutlinedButton}"
                           Height="50" 
                           Margin="0,0,0,10"
                           Command="{Binding NavigateCommand}"
                           CommandParameter="Users"
                           HorizontalAlignment="Stretch">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Border Background="{TemplateBinding Background}"
                                       BorderBrush="{TemplateBinding BorderBrush}"
                                       BorderThickness="1"
                                       CornerRadius="8">
                                    <StackPanel Orientation="Horizontal" 
                                              HorizontalAlignment="Center"
                                              VerticalAlignment="Center">
                                        <materialDesign:PackIcon Kind="AccountGroup" 
                                                                Width="20" Height="20" 
                                                                Margin="0,0,10,0"/>
                                        <TextBlock Text="{TemplateBinding Content}" 
                                                  FontSize="14" 
                                                  FontWeight="Medium"/>
                                    </StackPanel>
                                </Border>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>
                    
                    <!-- Admin Section -->
                    <TextBlock x:Name="AdminSectionText"
                             Text="إدارة النظام"
                             FontSize="14" 
                             FontWeight="Bold" 
                             Foreground="#757575" 
                             Margin="0,30,0,15"
                             Visibility="{Binding IsAdmin, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                    
                    <Button x:Name="ReportsButton"
                           Content="التقارير"
                           Style="{StaticResource MaterialDesignOutlinedButton}"
                           Height="50" 
                           Margin="0,0,0,10"
                           Command="{Binding NavigateCommand}"
                           CommandParameter="Reports"
                           HorizontalAlignment="Stretch"
                           Visibility="{Binding IsAdmin, Converter={StaticResource BooleanToVisibilityConverter}}">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Border Background="{TemplateBinding Background}"
                                       BorderBrush="{TemplateBinding BorderBrush}"
                                       BorderThickness="1"
                                       CornerRadius="8">
                                    <StackPanel Orientation="Horizontal" 
                                              HorizontalAlignment="Center"
                                              VerticalAlignment="Center">
                                        <materialDesign:PackIcon Kind="ChartLine" 
                                                                Width="20" Height="20" 
                                                                Margin="0,0,10,0"/>
                                        <TextBlock Text="{TemplateBinding Content}" 
                                                  FontSize="14" 
                                                  FontWeight="Medium"/>
                                    </StackPanel>
                                </Border>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>
                    
                    <Button x:Name="SettingsButton"
                           Content="الإعدادات"
                           Style="{StaticResource MaterialDesignOutlinedButton}"
                           Height="50" 
                           Margin="0,0,0,10"
                           Command="{Binding NavigateCommand}"
                           CommandParameter="Settings"
                           HorizontalAlignment="Stretch">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Border Background="{TemplateBinding Background}"
                                       BorderBrush="{TemplateBinding BorderBrush}"
                                       BorderThickness="1"
                                       CornerRadius="8">
                                    <StackPanel Orientation="Horizontal" 
                                              HorizontalAlignment="Center"
                                              VerticalAlignment="Center">
                                        <materialDesign:PackIcon Kind="Settings" 
                                                                Width="20" Height="20" 
                                                                Margin="0,0,10,0"/>
                                        <TextBlock Text="{TemplateBinding Content}" 
                                                  FontSize="14" 
                                                  FontWeight="Medium"/>
                                    </StackPanel>
                                </Border>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>
                    
                    <!-- Logout Button -->
                    <Separator Margin="0,30,0,15"/>
                    
                    <Button x:Name="LogoutButton"
                           Content="تسجيل الخروج"
                           Style="{StaticResource MaterialDesignRaisedButton}"
                           Height="50" 
                           Margin="0,0,0,10"
                           Background="#F44336"
                           Command="{Binding LogoutCommand}"
                           HorizontalAlignment="Stretch">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Border Background="{TemplateBinding Background}"
                                       BorderBrush="{TemplateBinding BorderBrush}"
                                       BorderThickness="1"
                                       CornerRadius="8">
                                    <StackPanel Orientation="Horizontal" 
                                              HorizontalAlignment="Center"
                                              VerticalAlignment="Center">
                                        <materialDesign:PackIcon Kind="Logout" 
                                                                Width="20" Height="20" 
                                                                Margin="0,0,10,0"/>
                                        <TextBlock Text="{TemplateBinding Content}" 
                                                  FontSize="14" 
                                                  FontWeight="Medium"/>
                                    </StackPanel>
                                </Border>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>
                </StackPanel>
            </ScrollViewer>
        </Border>
        
        <!-- Main Content Area -->
        <ContentControl x:Name="MainContentControl"
                      Grid.Column="1" 
                      Grid.Row="1" 
                      Margin="30">
            <ContentControl.Content>
                <!-- Dashboard View -->
                <Grid x:Name="DashboardView">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    
                    <!-- Status Cards -->
                    <ItemsControl Grid.Row="0">
                        <ItemsControl.ItemsPanel>
                            <ItemsPanelTemplate>
                                <UniformGrid Columns="4" Rows="1" Margin="0,0,0,30"/>
                            </ItemsPanelTemplate>
                        </ItemsControl.ItemsPanel>
                        <ItemsControl.Items>
                            <materialDesign:Card Style="{StaticResource StatusCardStyle}">
                                <StackPanel>
                                    <materialDesign:PackIcon Kind="Phone" 
                                                            Width="40" Height="40" 
                                                            Foreground="#4CAF50" 
                                                            HorizontalAlignment="Center"/>
                                    <TextBlock x:Name="TotalCallsTextBlock"
                                             Text="{Binding TodayStatistics.TotalCalls}"
                                             FontSize="24" 
                                             FontWeight="Bold" 
                                             HorizontalAlignment="Center" 
                                             Margin="0,10,0,5"/>
                                    <TextBlock Text="إجمالي المكالمات اليوم" 
                                              FontSize="14" 
                                              HorizontalAlignment="Center" 
                                              Foreground="#757575"/>
                                </StackPanel>
                            </materialDesign:Card>
                            
                            <materialDesign:Card Style="{StaticResource StatusCardStyle}">
                                <StackPanel>
                                    <materialDesign:PackIcon Kind="PhoneIncoming" 
                                                            Width="40" Height="40" 
                                                            Foreground="#2196F3" 
                                                            HorizontalAlignment="Center"/>
                                    <TextBlock x:Name="IncomingCallsTextBlock"
                                             Text="{Binding TodayStatistics.IncomingCalls}"
                                             FontSize="24" 
                                             FontWeight="Bold" 
                                             HorizontalAlignment="Center" 
                                             Margin="0,10,0,5"/>
                                    <TextBlock Text="المكالمات الواردة" 
                                              FontSize="14" 
                                              HorizontalAlignment="Center" 
                                              Foreground="#757575"/>
                                </StackPanel>
                            </materialDesign:Card>
                            
                            <materialDesign:Card Style="{StaticResource StatusCardStyle}">
                                <StackPanel>
                                    <materialDesign:PackIcon Kind="PhoneOutgoing" 
                                                            Width="40" Height="40" 
                                                            Foreground="#FF9800" 
                                                            HorizontalAlignment="Center"/>
                                    <TextBlock x:Name="OutgoingCallsTextBlock"
                                             Text="{Binding TodayStatistics.OutgoingCalls}"
                                             FontSize="24" 
                                             FontWeight="Bold" 
                                             HorizontalAlignment="Center" 
                                             Margin="0,10,0,5"/>
                                    <TextBlock Text="المكالمات الصادرة" 
                                              FontSize="14" 
                                              HorizontalAlignment="Center" 
                                              Foreground="#757575"/>
                                </StackPanel>
                            </materialDesign:Card>
                            
                            <materialDesign:Card Style="{StaticResource StatusCardStyle}">
                                <StackPanel>
                                    <materialDesign:PackIcon Kind="AccountCheck" 
                                                            Width="40" Height="40" 
                                                            Foreground="#9C27B0" 
                                                            HorizontalAlignment="Center"/>
                                    <TextBlock x:Name="OnlineUsersTextBlock"
                                             Text="{Binding OnlineUsersCount}"
                                             FontSize="24" 
                                             FontWeight="Bold" 
                                             HorizontalAlignment="Center" 
                                             Margin="0,10,0,5"/>
                                    <TextBlock Text="المستخدمون المتصلون" 
                                              FontSize="14" 
                                              HorizontalAlignment="Center" 
                                              Foreground="#757575"/>
                                </StackPanel>
                            </materialDesign:Card>
                        </ItemsControl.Items>
                    </ItemsControl>
                    
                    <!-- Charts and Details -->
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        
                        <!-- Call Statistics Chart -->
                        <materialDesign:Card Grid.Column="0" 
                                           Grid.Row="0"
                                           Style="{StaticResource StatusCardStyle}"
                                           Margin="0,0,15,0">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                
                                <TextBlock Text="إحصائيات المكالمات" 
                                          FontSize="16" 
                                          FontWeight="Bold" 
                                          Margin="0,0,0,15"/>
                                
                                <lvc:CartesianChart Grid.Row="1" 
                                                  Series="{Binding CallStatisticsSeries}"
                                                  DataTooltip="{x:Null}">
                                    <lvc:CartesianChart.AxisX>
                                        <lvc:Axis Title="الأيام" Labels="السبت,الأحد,الاثنين,الثلاثاء,الأربعاء,الخميس,الجمعة"/>
                                    </lvc:CartesianChart.AxisX>
                                    <lvc:CartesianChart.AxisY>
                                        <lvc:Axis Title="عدد المكالمات"/>
                                    </lvc:CartesianChart.AxisY>
                                </lvc:CartesianChart>
                            </Grid>
                        </materialDesign:Card>
                        
                        <!-- User Status List -->
                        <materialDesign:Card Grid.Column="1" 
                                           Grid.Row="0"
                                           Style="{StaticResource StatusCardStyle}"
                                           Margin="15,0,0,0">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                
                                <TextBlock Text="حالة المستخدمين" 
                                          FontSize="16" 
                                          FontWeight="Bold" 
                                          Margin="0,0,0,15"/>
                                
                                <ScrollViewer Grid.Row="1" 
                                            VerticalScrollBarVisibility="Auto">
                                    <ItemsControl ItemsSource="{Binding OnlineUsers}">
                                        <ItemsControl.ItemTemplate>
                                            <DataTemplate>
                                                <Border Padding="15,10" 
                                                      BorderBrush="#E0E0E0" 
                                                      BorderThickness="0,0,0,1">
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="Auto"/>
                                                        </Grid.ColumnDefinitions>
                                                        
                                                        <materialDesign:PackIcon Grid.Column="0"
                                                                              Kind="AccountCircle"
                                                                              Width="30" Height="30"
                                                                              Foreground="#607D8B"
                                                                              Margin="0,0,15,0"/>
                                                        
                                                        <StackPanel Grid.Column="1">
                                                            <TextBlock Text="{Binding FullName}"
                                                                     FontSize="14"
                                                                     FontWeight="Medium"/>
                                                            <TextBlock Text="{Binding Department}"
                                                                     FontSize="12"
                                                                     Foreground="#757575"/>
                                                        </StackPanel>
                                                        
                                                        <StackPanel Grid.Column="2" 
                                                                  HorizontalAlignment="Right">
                                                            <TextBlock Text="{Binding Status}"
                                                                     Style="{StaticResource CallStatusStyle}"
                                                                     FontSize="12"
                                                                     FontWeight="Medium"/>
                                                            <TextBlock Text="{Binding Extension}"
                                                                     FontSize="12"
                                                                     Foreground="#757575"
                                                                     HorizontalAlignment="Right"/>
                                                        </StackPanel>
                                                    </Grid>
                                                </Border>
                                            </DataTemplate>
                                        </ItemsControl.ItemTemplate>
                                    </ItemsControl>
                                </ScrollViewer>
                            </Grid>
                        </materialDesign:Card>
                        
                        <!-- Recent Activity -->
                        <materialDesign:Card Grid.ColumnSpan="2" 
                                           Grid.Row="1"
                                           Style="{StaticResource StatusCardStyle}"
                                           Margin="0,15,0,0">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                
                                <TextBlock Text="النشاطات الأخيرة" 
                                          FontSize="16" 
                                          FontWeight="Bold" 
                                          Margin="0,0,0,15"/>
                                
                                <ScrollViewer Grid.Row="1" 
                                            VerticalScrollBarVisibility="Auto">
                                    <ItemsControl ItemsSource="{Binding RecentActivities}">
                                        <ItemsControl.ItemTemplate>
                                            <DataTemplate>
                                                <Border Padding="15,10" 
                                                      BorderBrush="#E0E0E0" 
                                                      BorderThickness="0,0,0,1">
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="Auto"/>
                                                        </Grid.ColumnDefinitions>
                                                        
                                                        <materialDesign:PackIcon Grid.Column="0"
                                                                              Kind="{Binding Icon}"
                                                                              Width="20" Height="20"
                                                                              Foreground="#2196F3"
                                                                              Margin="0,0,15,0"/>
                                                        
                                                        <StackPanel Grid.Column="1">
                                                            <TextBlock Text="{Binding Action}"
                                                                     FontSize="14"
                                                                     FontWeight="Medium"/>
                                                            <TextBlock Text="{Binding Description}"
                                                                     FontSize="12"
                                                                     Foreground="#757575"/>
                                                        </StackPanel>
                                                        
                                                        <TextBlock Grid.Column="2"
                                                                 Text="{Binding Timestamp, StringFormat='{}{0:HH:mm}'}"
                                                                 FontSize="12"
                                                                 Foreground="#757575"
                                                                 HorizontalAlignment="Right"/>
                                                    </Grid>
                                                </Border>
                                            </DataTemplate>
                                        </ItemsControl.ItemTemplate>
                                    </ItemsControl>
                                </ScrollViewer>
                            </Grid>
                        </materialDesign:Card>
                    </Grid>
                </Grid>
                
                <!-- Other views will be loaded here -->
            </ContentControl.Content>
        </ContentControl>
        
        <!-- Status Bar -->
        <Border Grid.ColumnSpan="2" 
                Grid.Row="2"
                Background="#F5F5F5" 
                BorderBrush="#E0E0E0" 
                BorderThickness="0,1,0,0" 
                Height="30">
            <Grid Margin="30,0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                
                <TextBlock Grid.Column="0"
                         Text="{Binding ConnectionStatus}"
                         FontSize="12"
                         VerticalAlignment="Center"
                         Foreground="#757575"/>
                
                <TextBlock Grid.Column="1"
                         Text="Basra Delight Group - البصرة، العراق"
                        FontFamily="Tajawal"
                         FontSize="12"
                         VerticalAlignment="Center"
                         HorizontalAlignment="Center"
                         Foreground="#757575"/>
                
                <TextBlock Grid.Column="2"
                         Text="{Binding CurrentTime, StringFormat='{}{0:yyyy-MM-dd HH:mm:ss}'}"
                         FontSize="12"
                         VerticalAlignment="Center"
                         HorizontalAlignment="Right"
                         Foreground="#757575"/>
            </Grid>
        </Border>
    </Grid>
</Window>